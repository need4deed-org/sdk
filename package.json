{
  "name": "need4deed-sdk",
  "version": "0.0.10",
  "description": "Need4Deed.org SDK",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:ts": "tsc",
    "build:css": "copyfiles -u 1 \"src/styles/**/*.css\" dist/",
    "build": "yarn build:ts && yarn build:css",
    "watch": "tsc --watch --project tsconfig.cjs.json",
    "prepublishOnly": "yarn build",
    "publish": "yarn publish",
    "publish:npm": "yarn publish --access public"
  },
  "keywords": [
    "sdk",
    "react",
    "typescript",
    "api-hooks",
    "ui-components",
    "layouts",
    "styling",
    "utilities"
  ],
  "author": "Need4Deed.org",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/need4deed-org/sdk.git"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "copyfiles": "^2.4.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
